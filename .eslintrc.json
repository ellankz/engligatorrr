{
  "env": {
      "browser": true,
      "es2020": true
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "ts": "never"
        }
    ]
   },
  "plugins": [
    "import"
  ]
}
